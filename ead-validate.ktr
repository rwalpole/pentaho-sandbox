        <class_name>Processor</class_name>
        <class_source>public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
  if (first) {
    first = false;
  }

  Object[] r = getRow();

  if (r == null) {
    setOutputDone();
    return false;
  }
  r = createOutputRow(r, data.outputRowMeta.size());

  final String scopeContentXml = get(Fields.In, "scope_and_content").getString(r);
  final String eadPrefixXml = "&lt;?xml version=\"1.0\" encoding=\"UTF-8\"?>&lt;ead xmlns:xlink=\"http://www.w3.org/1999/xlink\" xmlns=\"urn:isbn:1-931666-22-9\" xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\">&lt;eadheader>&lt;eadid>&lt;/eadid>&lt;filedesc>&lt;titlestmt>&lt;titleproper/>&lt;titleproper/>&lt;/titlestmt>&lt;/filedesc>&lt;/eadheader>&lt;archdesc level=\"class\">&lt;did>&lt;langmaterial/>&lt;langmaterial/>&lt;/did>";
  final String eadSuffixXml = "&lt;/archdesc>&lt;/ead>";
  final String eadXml = eadPrefixXml + scopeContentXml + eadSuffixXml; 

  get(Fields.Out, "ead_xml").setValue(r, eadXml);

  putRow(data.outputRowMeta, r);

  return true;
}
</class_source>
